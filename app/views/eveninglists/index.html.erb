<h1>Listing eveninglists</h1>

<table>
  <tr>
    <th>Note</th>
    <th>Status</th>
    <th>Day</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @eveninglists.each do |eveninglist| %>
  <tr>
    <td><%= eveninglist.note %></td>
    <td><%= eveninglist.status %></td>
    <td><%= eveninglist.day_id %></td>
    <td><%= link_to 'Show', eveninglist %></td>
    <td><%= link_to 'Edit', edit_eveninglist_path(eveninglist) %></td>
    <td><%= link_to 'Destroy', eveninglist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Eveninglist', new_eveninglist_path %>
